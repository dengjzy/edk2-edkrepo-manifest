<Manifest>
  <ProjectInfo>
    <CodeName>EdkRepo</CodeName>
    <Description>EdkRepo - The Multi-Repository Tool for EDK II</Description>
    <DevLead>ashley.e.desimone@intel.com</DevLead>
    <LeadReviewers>
      <Reviewer>ashley.e.desimone@intel.com</Reviewer>
      <Reviewer>nathaniel.l.desimone@intel.com</Reviewer>
    </LeadReviewers>
  </ProjectInfo>

  <GeneralConfig>
    <PinPath>edk2/EdkRepo/pins</PinPath>
    <DefaultCombo combination="main" />
    <CurrentClonedCombo combination="main" />
  </GeneralConfig>

  <RemoteList>
    <Remote name="Edk2EdkRepoRepo">https://github.com/tianocore/edk2-edkrepo.git</Remote>
  </RemoteList>

  <ClientGitHookList>
  </ClientGitHookList>

  <CommitTemplates>
    <Template remoteName="Edk2EdkRepoRepo">Templates/edkrepo-review-template.txt</Template>
  </CommitTemplates>

  <CombinationList>
    <Combination name="main" description="the main branch from all repos">
      <Source localRoot="edk2-edkrepo" remote="Edk2EdkRepoRepo" branch="main" />
    </Combination>
  </CombinationList>

  <DscList>
  </DscList>
</Manifest>
